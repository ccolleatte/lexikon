# Database Configuration
# Set these values in .env.local for local development
# Use strong, random passwords in production
POSTGRES_PASSWORD=change-me-in-production
NEO4J_PASSWORD=change-me-in-production

# JWT Configuration
JWT_SECRET=change-me-in-production

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:5175,http://localhost:3000

# Environment
ENVIRONMENT=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Backend URL
BACKEND_URL=http://localhost:8000
